{
  "manifest_version": 3,
  "name": "Ethereum Gwei Checker",
  "version": "1.1",
  "description": "Simple Ethereum Gwei Checker, 10 seconds upd.",
  "permissions": ["alarms", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
